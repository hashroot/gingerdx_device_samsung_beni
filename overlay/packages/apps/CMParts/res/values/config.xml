<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Whether or not to display the trackball settings -->
    <bool name="has_trackball">false</bool>
    
    <!-- Whether or not to display the camera button settings -->
	<bool name="has_camera_button">true</bool>
	
    <!-- Notification LED can show any color -->
    <bool name="has_rgb_notification_led">true</bool>
    
    <!-- LED Flashlight -->
    <bool name="has_led_flash">true</bool>
    
    <!-- Whether or not to display search button settings -->
    <bool name="has_search_button">false</bool>
    
    <!-- Undervolting kernel module -->
    <add-resource type="string" name="undervolting_module">x10miniprouv.ko</add-resource>
</resources>
